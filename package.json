{
  "name": "@ninepay/nfse-pdf-generator",
  "version": "1.0.5",
  "description": "Gerador de PDF para NFS-e Nacional (Nota Fiscal de Serviços Eletrônica) seguindo o padrão da Nota Fiscal Nacional",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nfse",
    "nfs-e",
    "nfse-nacional",
    "pdf",
    "pdf-generator",
    "invoice",
    "nota-fiscal",
    "nota-fiscal-servicos",
    "danfse",
    "xml-to-pdf",
    "brazil",
    "brasil",
    "fiscal"
  ],
  "author": "Arthur Fonseca",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurbfonseca27/nfse-pdf-generator.git"
  },
  "bugs": {
    "url": "https://github.com/arthurbfonseca27/nfse-pdf-generator/issues"
  },
  "homepage": "https://github.com/arthurbfonseca27/nfse-pdf-generator#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.5",
    "image-size": "^2.0.2",
    "pdfkit": "^0.15.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pdfkit": "^0.13.5",
    "@types/qrcode": "^1.5.6",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
